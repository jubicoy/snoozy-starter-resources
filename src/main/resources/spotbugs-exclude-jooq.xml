<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Package name="~.*\.db" />
    </Match>
    <Match>
        <Package name="~.*\.db\.tables" />
    </Match>
    <Match>
        <Package name="~.*\.db\.tables\.records" />
    </Match>
    <Match>
        <Package name="~.*\.db\.public_" />
    </Match>
    <Match>
        <Package name="~.*\.db\.public_\.tables" />
    </Match>
    <Match>
        <Package name="~.*\.db\.public_\.tables\.records" />
    </Match>
</FindBugsFilter>